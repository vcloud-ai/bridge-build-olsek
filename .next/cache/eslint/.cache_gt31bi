sLoggedIn(true);\n          setLogin(login);\n        }\n      })\n      .catch((err) => {\n        router.push(\"/login\");\n      })\n      .finally(() => {\n        setIsAuthInfoLoading(false);\n      });\n  }, []);\n\n  const onLoginChange = (e) => {\n    setLogin(e.target.value.trim());\n  };\n  const onPasswordChange = (e) => {\n    setPassword(e.target.value.trim());\n  };\n\n  const onLogin = async (e) => {\n    e.preventDefault();\n\n    const userInput = {\n      login,\n      password,\n    };\n\n    try {\n      setIsLoading(true);\n      const cloudLogin = await connectToCloud(userInput);\n      if (!cloudLogin) return;\n\n      setLogin(cloudLogin);\n      setIsLoggedIn(true);\n    } catch (error) {\n      router.push(\"/login\");\n    } finally {\n      setIsLoading(false);\n    }\n  };\n\n  const onLogout = async (e) => {\n    e.preventDefault();\n\n    try {\n      setIsLoading(true);\n      await logoutFromCloud();\n      setIsLoggedIn(false);\n    } catch (error) {\n      router.push(\"/login\");\n    } finally {\n      setIsLoading(false);\n      setPassword(\"\");\n    }\n  };\n\n  if (isAuthInfoLoading) return <AppLoader />;\n\n  return (\n    <Form onSubmit={isLoggedIn ? onLogout : onLogin}>\n      <InputWrapper>\n        <Label>{isLoggedIn ? \"Logged in as\" : \"Login\"}</Label>\n        <Input\n          isLoggedIn={isLoggedIn}\n          value={login}\n          type=\"login\"\n          required\n          onChange={onLoginChange}\n          disabled={isLoggedIn}\n        />\n      </InputWrapper>\n      {!isLoggedIn && (\n        <InputWrapper>\n          <Label>Password</Label>\n          <Input\n            value={password}\n            type=\"password\"\n            required\n            onChange={onPasswordChange}\n          />\n        </InputWrapper>\n      )}\n\n      {isLoading ? (\n        <AppLoader />\n      ) : (\n        <Button type=\"submit\">{isLoggedIn ? \"Log out\" : \"Login\"}</Button>\n      )}\n    </Form>\n  );\n};\n\nexport default CloudLoginForm;\n","/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/parts/CloudLoginForm/styles.js",[],[],"/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/parts/IpChangeForm/index.js",["307"],[],"import React, { useState, useEffect } from \"react\";\nimport { Form, InputWrapper, Label, Input, Button } from \"./styles\";\nimport { updateIpAddress, getIpAddress } from \"services/requests\";\nimport toast from \"components/common/Toast\";\nimport AppLoader from \"components/common/AppLoader\";\nimport { useRouter } from \"next/router\";\n\nconst IpChangeForm = () => {\n  const router = useRouter();\n  const [isLoading, setIsLoading] = useState(false);\n  const [ip, setIp] = useState(\"\");\n  const [isLoadingAddress, setIsLoadingAddress] = useState(true);\n\n  useEffect(() => {\n    setIsLoadingAddress(true);\n    getIpAddress()\n      .then(setIp)\n      .catch((err) => {\n        router.push(\"/login\");\n      })\n      .finally(() => {\n        setIsLoadingAddress(false);\n      });\n  }, []);\n\n  const onIpChange = (e) => {\n    setIp(e.target.value);\n  };\n\n  const onSubmit = async (e) => {\n    e.preventDefault();\n\n    const ipCheck =\n      /^((25[0-5]|(2[0-4]|1\\d|[1-9]|)\\d)\\.){3}(25[0-5]|(2[0-4]|1\\d|[1-9]|)\\d)$/;\n    const isValid = ipCheck.test(ip);\n    if (\n      !ip.includes(\"192.168.\") ||\n      !isValid ||\n      ip.endsWith(\".0\") ||\n      ip.endsWith(\".1\")\n    ) {\n      toast({\n        type: \"error\",\n        message: \"Invalid format\",\n      });\n      return;\n    }\n\n    try {\n      setIsLoading(true);\n      await updateIpAddress(ip);\n    } catch (error) {\n      router.push(\"/login\");\n    } finally {\n      setIsLoading(false);\n    }\n  };\n\n  if (isLoadingAddress) return <AppLoader />;\n\n  return (\n    <Form onSubmit={onSubmit}>\n      <InputWrapper>\n        <Label>Enter new IP address. Address change will reboot bridge.</Label>\n        <Input value={ip} type=\"text\" required onChange={onIpChange} />\n      </InputWrapper>\n\n      {isLoading ? <AppLoader /> : <Button type=\"submit\">Submit</Button>}\n    </Form>\n  );\n};\n\nexport default IpChangeForm;\n","/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/parts/IpChangeForm/styles.js",[],[],"/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/parts/PasswordChangeForm/index.js",[],[],"/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/parts/PasswordChangeForm/styles.js",[],[],"/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/SubSettings/styles.js",[],[],"/Users/apple/Documents/projects/vcloud-repos/orange-finder/components/settings/styles.js",[],[],{"ruleId":"308","severity":1,"message":"309","line":41,"column":6,"nodeType":"310","endLine":41,"endColumn":8,"suggestions":"311"},{"ruleId":"308","severity":1,"message":"309","line":54,"column":6,"nodeType":"310","endLine":54,"endColumn":21,"suggestions":"312"},{"ruleId":"308","severity":1,"message":"309","line":19,"column":6,"nodeType":"310","endLine":19,"endColumn":8,"suggestions":"313"},{"ruleId":"308","severity":1,"message":"309","line":26,"column":6,"nodeType":"310","endLine":26,"endColumn":8,"suggestions":"314"},{"ruleId":"308","severity":1,"message":"309","line":33,"column":6,"nodeType":"310","endLine":33,"endColumn":8,"suggestions":"315"},{"ruleId":"308","severity":1,"message":"309","line":24,"column":6,"nodeType":"310","endLine":24,"endColumn":8,"suggestions":"316"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'router'. Either include it or remove the dependency array.","ArrayExpression",["317"],["318"],["319"],["320"],["321"],["322"],{"desc":"323","fix":"324"},{"desc":"325","fix":"326"},{"desc":"323","fix":"327"},{"desc":"323","fix":"328"},{"desc":"323","fix":"329"},{"desc":"323","fi